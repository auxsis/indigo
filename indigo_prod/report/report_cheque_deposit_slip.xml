                                      <t t-set="total" t-value="total + account_cheque_id.amount"/>
                                  </t>
                            </tbody>
                          </table>
                        <span t-esc="'{0:,.2f}'.format(total)" style="top: 116mm; left: 265mm; position:absolute;"/>  
                      </div>
                  </t>
                </t>
              </t>                   
            
